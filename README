- ClassificationChoice.rb
- ClassifyInterestingWords.rb
- ClassifyLoanWords.rb
- CountFrequencies.rb
- CountWords.rb
- FilterUniqueWords.rb
- FindOccurrences.rb
- Helpers.rb
- WordClass.rb
- WordClassifier.rb
